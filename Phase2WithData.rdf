    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/2015/02/semcloth#dressingCodeHasSuitableClothingForDressing">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/ontologies/2015/02/semcloth#isSuitableForDressingCode"/>
