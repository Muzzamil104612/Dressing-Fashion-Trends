    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth#fashionBrandRelatedTo -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/2015/02/semcloth#fashionBrandRelatedTo">
