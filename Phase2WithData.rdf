    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth#hasRelatedEvent -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/2015/02/semcloth#hasRelatedEvent">
