    <!-- http://dbpedia.org/property/dressSize -->

    <owl:ObjectProperty rdf:about="http://dbpedia.org/property/dressSize">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/ontologies/2015/02/semcloth#hasSize"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/ontologies/2015/02/semcloth#isDressSizeOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2015/02/semcloth#Dress"/>
        <rdfs:range rdf:resource="http://dbpedia.org/resource/Clothing_sizes"/>
        <rdfs:label>dbp:dressSize</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/partof.owl#hasPart -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/cp/owl/partof.owl#hasPart">
        <owl:inverseOf rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/partof.owl#isPartOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2015/02/semcloth#Dress"/>
        <rdfs:range rdf:resource="http://dbpedia.org/resource/Clothing"/>
        <rdfs:label>hasPart</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.ontologydesignpatterns.org/cp/owl/partof.owl#isPartOf -->

    <owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/cp/owl/partof.owl#isPartOf">
        <rdfs:domain rdf:resource="http://dbpedia.org/resource/Clothing"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/2015/02/semcloth#Dress"/>
        <rdfs:label>isPartOf</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth#associatedWithFashionTrend -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/2015/02/semcloth#associatedWithFashionTrend">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/ontologies/2015/02/semcloth#isAssociatedWithByFashionTrend"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://dbpedia.org/ontology/Colour"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/ontologies/2015/02/semcloth#AgeGroup"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/2015/02/semcloth#FashionTrend"/>
        <rdfs:label>associatedWithFashionTrend</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth#belongsTo -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/2015/02/semcloth#belongsTo">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/ontologies/2015/02/semcloth#hasBelonging"/>
        <rdfs:domain rdf:resource="https://schema.org/Person#Person"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/2015/02/semcloth#AgeGroup"/>
        <rdfs:label>belongsTo</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth#bodyShapeHasSuitableClothingForDressing -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/2015/02/semcloth#bodyShapeHasSuitableClothingForDressing">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/ontologies/2015/02/semcloth#isSuitableToBeDressedByBodyShape"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2015/02/semcloth#BodyShape"/>
        <rdfs:range rdf:resource="http://dbpedia.org/resource/Clothing"/>
        <rdfs:comment xml:lang="en">This property specifies the clothing item(s) that is/are suitable for a particular body shape.</rdfs:comment>
        <rdfs:label>bodyShapeHasSuitableClothingForDressing</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth#browsingHistoryUtilizedBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/2015/02/semcloth#browsingHistoryUtilizedBy">
