        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2015/02/semcloth#RecommendationSystem"/>
        <rdfs:range rdf:resource="https://schema.org/Person#Person"/>
        <rdfs:label>generatesRecommendationsFor</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth#hasBelonging -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/2015/02/semcloth#hasBelonging">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2015/02/semcloth#AgeGroup"/>
        <rdfs:range rdf:resource="https://schema.org/Person#Person"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth#hasColour -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/2015/02/semcloth#hasColour">
