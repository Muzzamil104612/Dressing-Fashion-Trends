        <rdfs:label>dressingCodeHasSuitableClothingForDressing</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth#eventHasSuitableClothingForDressing -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/2015/02/semcloth#eventHasSuitableClothingForDressing">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/ontologies/2015/02/semcloth#isSuitableToBeDressedAtEvent"/>
        <rdfs:domain rdf:resource="http://dbpedia.org/ontology/Event"/>
