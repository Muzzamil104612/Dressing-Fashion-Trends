        <rdfs:domain rdf:resource="http://www.semanticweb.org/ontologies/2015/02/semcloth#BrowsingHistory"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/ontologies/2015/02/semcloth#RecommendationSystem"/>
        <rdfs:label>browsingHistoryUtilizedBy</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth#budgetDetailsUtilizedBy -->

