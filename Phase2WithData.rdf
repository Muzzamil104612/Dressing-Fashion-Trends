    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/ontologies/2015/02/semcloth#hasRecommendedDress -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/2015/02/semcloth#hasRecommendedDress">
