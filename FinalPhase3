    <owl:Class rdf:about="http://dbpedia.org/resource/Clothing">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/ontologies/2015/02/semcloth.owl#hasColour"/>
                <owl:someValuesFrom rdf:resource="http://dbpedia.org/ontology/Colour"/>
            </owl:Restriction>
        </owl:equivalentClass>
